{
  "plugins": [
    "react",
    "jsx-a11y"
  ],
  "env": {
    "browser": true,
    "es2021": true
},
  "extends": [
    "airbnb",
    "airbnb/hooks",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:react/jsx-runtime"
],
  "rules": {
    "no-console": "off",
    "semi": [ "error", "never" ],
    "react/prop-types": 0
  }
}